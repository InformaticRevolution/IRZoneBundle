<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  
    <parameters>
        <parameter key="ir_zone.manager.region.class">IR\Bundle\ZoneBundle\Doctrine\RegionManager</parameter>
    </parameters>

    <services>        
        <service id="ir_zone.manager.region.default" class="%ir_zone.manager.region.class%" public="false">
            <argument type="service" id="ir_zone.entity_manager" />
            <argument>%ir_zone.model.region.class%</argument>
        </service>          
    </services>  

</container>
