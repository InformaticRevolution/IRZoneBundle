<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="ir_zone_admin_zone_list" pattern="/" methods="GET">
        <default key="_controller">IRZoneBundle:Admin/Zone:list</default>
    </route>
 
    <route id="ir_zone_admin_zone_new" pattern="/new">
        <default key="_controller">IRZoneBundle:Admin/Zone:new</default>
    </route>

    <route id="ir_zone_admin_zone_show" pattern="/{id}" methods="GET">
        <default key="_controller">IRZoneBundle:Admin/Zone:show</default>
    </route>

    <route id="ir_zone_admin_zone_edit" pattern="/{id}/edit">
        <default key="_controller">IRZoneBundle:Admin/Zone:edit</default>
    </route>   
    
    <route id="ir_zone_admin_zone_delete" pattern="/{id}/delete" methods="GET">
        <default key="_controller">IRZoneBundle:Admin/Zone:delete</default>
    </route>
</routes>